{
    "name": "Data Attributes Validator",
    "description": "Data attributes validation tool made by Vincent",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "hello.html",
        "default_icon": "hello_extensions.png"
    },
    "content_scripts": [
      {
        "js": ["content.js"],
        "matches": [
            "http://*/*",
            "https://*/*"
        ]
      }
    ]
}